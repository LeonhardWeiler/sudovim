{
  "name": "sudoku",
  "version": "1.0.0",
  "description": "sudoku with vim motions in the browser",
  "main": "index.js",
  "scripts": {
    "minify-html": "html-minifier-terser src/index.html --collapse-whitespace --output=build/index.html",
    "optimize-css": "postcss src/style.css --use autoprefixer cssnano postcss-pxtorem --output build/style.css --no-map",
    "minify-js": "uglifyjs src/script.js -o build/script.js",
    "build": "npm run minify-html && npm run optimize-css && npm run minify-js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.2",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-pxtorem": "^6.1.0",
    "uglify-js": "^3.17.4"
  }
}
